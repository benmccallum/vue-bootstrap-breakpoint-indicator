{
  "name": "vue-bootstrap-breakpoint-indicator",
  "version": "2.0.0",
  "description": "A Vue component that displays an indicator of the current Bootstrap breakpoint (e.g. XS, SM, MD, LG or XL).",
  "keywords": [
    "vue",
    "bootstrap",
    "breakpoint",
    "indicator",
    "responsive",
    "debugging"
  ],
  "main": "dist/VueBootstrapBreakpointIndicator.common.js",
  "module": "dist/VueBootstrapBreakpointIndicator.esm.js",
  "unpkg": "dist/VueBootstrapBreakpointIndicator.umd.min.js",
  "scripts": {
    "build": "vue-cli-service build --target lib --name VueBootstrapBreakpointIndicator"
  },
  "author": "Ben McCallum",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benmccallum/vue-bootstrap-breakpoint-indicator"
  },
  "files": [
    "dist/*",
    "!dist/*.html",
    "src/*",
    "*.json",
    "*.md"
  ],
  "devDependencies": {
    "@vue/cli-service": "^4.2.3",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  }
}
